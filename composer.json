{
    "name": "yuga/sockets",
    "description": "The Yuga Framework Websockets Package.",
    "keywords": ["framework", "Yuga", "Websockets", "Sockets"],
    "license": "MIT",
    "homepage": "https://yuga-framework.gitbook.io/documentation/websockets",
    "authors": [
        {
            "name": "Hamidouh Semix",
            "email": "semix.hamidouh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "cboden/ratchet": "^0.4.1",
        "textalk/websocket": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "yuga/framework": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Yuga\\Sockets\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "yuga": {
            "providers": [
                "Yuga\\Providers\\SocketsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
